    <children>
        <HBox alignment="CENTER_LEFT" prefHeight="53.0" prefWidth="444.0" VBox.vgrow="NEVER">
            <children>
                <Button mnemonicParsing="false" onAction="#newRecord" text="Novo">
                    <HBox.margin>
                        <Insets left="10.0" right="10.0"/>
                    </HBox.margin>
                </Button>
                <Button fx:id="buttonEdit" mnemonicParsing="false" onAction="#edit" text="Editar">
                    <HBox.margin>
                        <Insets left="10.0" right="10.0"/>
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#delete" text="Remover">
                    <HBox.margin>
                        <Insets left="10.0" right="10.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <TableView fx:id="tableData" prefHeight="351.0" prefWidth="381.0" tableMenuButtonVisible="true"
                   VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="columnId" maxWidth="150.0" minWidth="100.0" prefWidth="150.0" text="CÃ³digo"/>
                <TableColumn fx:id="columnNome" maxWidth="-1.0" minWidth="300.0" prefWidth="-1.0" text="Nome"/>
            </columns>
        </TableView>
    </children>
</VBox>
